<div class="logout">
  <%= form_for @conn, Routes.login_path(@conn, :login), [as: :logout], fn f -> %>
      <%= submit "Logout", class: "button" %>
  <% end %>
</div>


<%= render GateGoatWeb.LookupView, "form.html", conn: @conn, error: @error %>

<span><%= link "Back", to: Routes.admin_path(@conn, :index) %></span>
